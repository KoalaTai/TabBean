{
  "manifest_version": 3,
  "name": "TabBean",
  "description": "A Intelligent Chrome extension to manage tabs efficiently.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popux.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https:/*/*", "http://*/*"],
      "js": ["content.js"]
    }
  ],
  "author": "KoalaTai",
  "homepage_url": "https://github.com/KoalaTai/TabBean",
  "options_page": "options.html"
}
